{
  "name": "positional-zod",
  "version": "0.1.1",
  "description": "Schema-driven positional format for LLM structured outputs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors.js",
      "types": "./dist/errors.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "demo": "tsx test.ts",
    "comparison": "tsx comparison.ts"
  },
  "keywords": [
    "zod",
    "llm",
    "openai",
    "anthropic",
    "gemini",
    "structured-output",
    "positional",
    "token-efficient"
  ],
  "author": "Duc Nguyen <danielbaker06072001@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ducnguyen67201/PositionalZod#readme",
  "bugs": {
    "url": "https://github.com/ducnguyen67201/PositionalZod/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ducnguyen67201/PositionalZod.git"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@google/generative-ai": "^0.21.0",
    "openai": "^4.0.0",
    "zod": "^3.22.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "@google/generative-ai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@toon-format/toon": "^2.1.0",
    "@types/node": "^22.0.0",
    "dotenv": "^17.2.3",
    "openai": "^6.15.0",
    "tsx": "^4.21.0",
    "zod": "^4.2.1"
  },
  "optionalDependencies": {
    "zod-v4": "npm:zod@^4.0.0-beta.0"
  }
}
